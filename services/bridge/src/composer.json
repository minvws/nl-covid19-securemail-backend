{
    "name": "minvws/securemail-bridge",
    "description": "Bridge for the Dutch COVID-19 Secure Mail project",
    "homepage": "https://github.com/minvws/nl-covid19-securemail-backend",
    "license": "EUPL/1.2",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.2",
        "laminas/laminas-config": "^3.5",
        "minvws/bridge-server": "dev-main",
        "monolog/monolog": "^2.1",
        "php-di/php-di": "^6.2",
        "predis/predis": "^1.1",
        "symfony/console": "^6.0"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.4",
        "slevomat/coding-standard": "^7.0",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/var-dumper": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "/shared/repositories/nl-covid19-bridge-private/packages/minvws/*"
        }
    ]
}
