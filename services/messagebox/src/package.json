{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "concurrently -n tsc,vetur,mix -c yellow,blue,magenta \"tsc\" \"npm run vetur\" \"mix\"",
        "watch": "concurrently --kill-others -n tsc,vetur,mix -c yellow,blue,magenta \"tsc --watch --pretty\" \"npm run vetur:watch\" \"mix watch\"",
        "hot": "concurrently --kill-others -n tsc,vetur,mix -c yellow,blue,magenta \"tsc --watch --pretty\" \"npm run vetur:watch\" \"mix watch --hot\"",
        "prod": "npm run production",
        "production": "tsc && npm run vetur && mix --production",
        "test:fe-unit": "jest",
        "test:coverage": "npx jest --coverage",
        "vetur": "node ./vti-diagnostics.js",
        "vetur:watch": "nodemon -q --watch resources -e ts,vue --exec npm run vetur"
    },
    "dependencies": {
        "bootstrap": "^4.6.1",
        "bootstrap-vue": "^2.23.1",
        "date-fns": "^2.28.0",
        "laravel-mix-svg-vue": "^0.4.1",
        "popper.js": "^1.16.1",
        "v-mask": "^2.3.0",
        "vee-validate": "^3.4.14",
        "vue-i18n": "^8.27.2",
        "vue-loader": "^15.9.5",
        "vue-router": "^3.5.3"
    },
    "devDependencies": {
        "@babel/core": "^7.20.12",
        "@babel/plugin-transform-runtime": "^7.21.4",
        "@babel/preset-typescript": "^7.18.6",
        "@faker-js/faker": "7.2.0",
        "@testing-library/dom": "8.14.0",
        "@testing-library/vue": "5.8.3",
        "@types/jest": "^27.4.1",
        "@types/webpack-env": "^1.16.3",
        "@vue/test-utils": "^1.3.0",
        "axios": "^0.21.4",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "^27.5.1",
        "concurrently": "^6.5.1",
        "core-js": "^3.21.1",
        "cross-env": "^7.0.3",
        "intersection-observer": "^0.12.2",
        "jest": "^27.5.1",
        "laravel-mix": "^6.0.49",
        "lodash": "^4.17.21",
        "nodemon": "^2.0.20",
        "postcss": "^8.4.31",
        "regenerator-runtime": "^0.13.11",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.49.8",
        "sass-loader": "^13.0.2",
        "ts-loader": "^9.3.1",
        "typescript": "^4.6.2",
